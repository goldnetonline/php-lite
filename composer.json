{
    "name": "goldnetonline/php-lite",
    "description": "Internal Brandnaware Framework for small websites.",
    "require": {
        "php": "^8.3",
        "twig/twig": "^3.3",
        "mailgun/mailgun-php": "^4.3",
        "vlucas/phpdotenv": "^5.6",
        "swiftmailer/swiftmailer": "^6.3",
        "kriswallsmith/buzz": "^1.3",
        "nyholm/psr7": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "require-dev": {
        "filp/whoops": "^2.17"
    }
}
